<%_
if (typeof linyaSub === 'undefined') var linyaSub = getvar('stat_data.林雅状态.屈从度', { defaults: 0 });
if (typeof linyaRel === 'undefined') var linyaRel = getvar('stat_data.林雅状态.关系', { defaults: '同学' });
_%>

<%_ if (linyaRel === '玩物' || linyaSub >= 81) { _%>
<%- await getwi(null, '林雅_阶段05_崩坏玩物期') %>
<%_ } else if (linyaSub >= 61) { _%>
<%- await getwi(null, '林雅_阶段04_恐惧服从期') %>
<%_ } else if (linyaSub >= 41) { _%>
<%- await getwi(null, '林雅_阶段03_被迫妥协期') %>
<%_ } else if (linyaSub >= 21) { _%>
<%- await getwi(null, '林雅_阶段02_震惊动摇期') %>
<%_ } else { _%>
<%- await getwi(null, '林雅_阶段01_傲慢施虐期') %>
<%_ } _%>
